{
  "storage": {
    "file": {
      "path": "/data/file"
    }
  },
  "listener": [
    {
      "tcp": {
        "address": "0.0.0.0:8200",
        "tls_cert_file": "/data/tls/cert.pem",
        "tls_key_file": "/data/tls/cert.key.pem"
      }
    }
  ],
  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h",
  "api_addr": "http://127.0.0.1:8200",
  "disable_mlock": true,
  "ui": true
}
